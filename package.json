    "lint:fix": "eslint --fix",
    "format": "prettier --config .prettierrc --write .",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "test:cov": "vitest run --coverage",
    "release": "semantic-release",
    "prepare": "husky"
